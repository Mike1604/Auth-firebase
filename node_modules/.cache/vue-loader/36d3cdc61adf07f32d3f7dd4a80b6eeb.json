avbar from '../components/Navbar.vue'\nimport { collection, getDocs } from 'firebase/firestore/lite';\nimport { db } from \"../main\";\nimport To_do from '../components/to_do.vue';\nexport default {\n    name: 'Home',\n    components: {\n    Navbar,\n    To_do\n},\n    data() {\n        return {\n          usuarios: [],\n          usuario: {\n            nombre: '',\n            correo: '',\n            foto: ''\n          }\n        }\n    },\n    methods:{\n  // GET\n    async obtenerDatos () { \n      const querySnapshot = await getDocs(collection(db, \"usuarios\"));\n        querySnapshot.forEach((doc) => {\n        let usuario = doc.data()\n        usuario.id = doc.id\n        this.usuarios.push(usuario)\n        console.log(usuario)\n      });\n    }\n    },\n    mounted() {\n        this.obtenerDatos();\n    },\n}\n</script>\n"]}]}